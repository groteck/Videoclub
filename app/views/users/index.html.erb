<h1>Listing users</h1>

<% @user.each do |user| %>
  <table>
	<tr>
           <td>
		<tr> Usuario </tr>
		<tr> Permiso de admin </tr>
	   </td> 
	   <td><%=user.email%></td>
	   <td><%= link_to 'destroy', user, confirm: 'Are you sure?', method: :delete %></td>
	   <td>
		<%= form_for user do |f| %>
			<%=f.check_box :group%>
			<%=f.submit%>
           	<% end %>
	   </td>
	</tr>
  </table>

<% end %>

